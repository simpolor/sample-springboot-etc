<?xml version="1.0" encoding="UTF-8"?>

<included>

    <!-- 콘솔창 로깅 출력 -->
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <withJansi>true</withJansi>
        <encoder>
            <Pattern>%d{yyyy-MMM-dd HH:mm:ss.SSS}[%-5level [%thread] %logger{15}.%M\(%line\) - %msg%n</Pattern>
        </encoder>
    </appender>


</included>